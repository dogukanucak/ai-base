{
  "embedding": {
    "modelName": "Xenova/all-MiniLM-L6-v2"
  },
  "vectorStore": {
    "type": "chroma",
    "collectionName": "ai_base",
    "similarityThreshold": 0.2
  },
  "documentLoader": {
    "type": "markdown",
    "path": "docs",
    "enabled": true
  },
  "openAI": {
    "model": "gpt-4-turbo-preview",
    "maxTokens": 1000,
    "temperature": 0.7,
    "enabled": true
  },
  "console": {
    "maxResponseLength": 1000,
    "showDebugInfo": false,
    "truncateDocuments": true,
    "documentPreviewLength": 1000
  }
}
