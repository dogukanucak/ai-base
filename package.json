{
  "name": "ai-base",
  "version": "1.0.0",
  "description": "Simple RAG system with local embeddings and vector storage",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "tsx examples/basic.ts",
    "start:openai": "tsx examples/openai-rag.ts",
    "start:server": "tsx src/server/index.ts",
    "dev": "tsx watch src/server/index.ts",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "chroma": "chroma run --path ./data/chroma --host localhost --port 8000",
    "university-agent": "tsx src/projects/UniversityAgent/examples/index.ts",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@huggingface/transformers": "^3.4.2",
    "@langchain/community": "^0.3.39",
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.63",
    "@langchain/openai": "^0.5.4",
    "@types/node-fetch": "^2.6.12",
    "cheerio": "^1.0.0",
    "chromadb": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "faiss-node": "^0.5.1",
    "fastify": "^5.2.2",
    "front-matter": "^4.0.2",
    "glob": "^11.0.1",
    "inversify": "^7.5.0",
    "langchain": "^0.3.19",
    "node-fetch": "^3.3.2",
    "openai": "^4.91.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.30",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "imports": {
    "@ai/*": "./src/ai/*",
    "@documents/*": "./src/documents/*",
    "@factory/*": "./src/factory/*",
    "@embeddings/*": "./src/embeddings/*",
    "@flow/*": "./src/flow/*",
    "@rag/*": "./src/rag/*",
    "@server/*": "./src/server/*",
    "@types/*": "./src/types/*",
    "@config/*": "./src/config/*"
  }
}
