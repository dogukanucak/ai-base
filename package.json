{
  "name": "ai-base",
  "version": "1.0.0",
  "private": true,
  "description": "Simple RAG system with local embeddings and vector storage",
  "scripts": {
    "start": "pnpm --filter @ai-base/server start",
    "start:openai": "pnpm --filter @ai-base/projects start:openai",
    "start:server": "pnpm --filter @ai-base/server start",
    "dev": "pnpm --filter @ai-base/server dev",
    "type-check": "pnpm -r type-check",
    "build": "pnpm -r build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "chroma": "chroma run --path ./data/chroma --host localhost --port 8000",
    "university-agent": "pnpm --filter @ai-base/projects university-agent",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "example:basic": "pnpm --filter @ai-base/core example:basic",
    "example:ai-rag": "pnpm --filter @ai-base/core example:ai-rag"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4"
  },
  "dependencies": {
    "pdf-parse": "^1.1.1"
  }
}
